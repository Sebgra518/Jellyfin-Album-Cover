#Jellyfin Option
JELLYFIN_URL=http://192.168.xxx.xxx:8096
JELLYFIN_USER=<username>
JELLYFIN_PASSWORD=<password>
# Optional: alternate URL if first fails (e.g., Tailscale)
JELLYFIN_FALLBACK_URL=http://100.xx.xx.xx:8096
# Matrix options
MATRIX_ROWS=64
MATRIX_COLS=64
MATRIX_CHAIN=2
MATRIX_PARALLEL=2
MATRIX_BRIGHTNESS=60
MATRIX_LIMIT_HZ=60
MATRIX_GPIO_SLOWDOWN=4
MATRIX_MULTIPLEXING=0
# Jellyfin Music Mount Point
MEDIA_MOUNT_POINT = <Your Mount Point>
#Navidrome
NAVIDROME_URL=http://<host>:<port>         # e.g. http://192.168.1.50:4533
NAVIDROME_USER=your-user
NAVIDROME_PASSWORD=your-password           # or app password
NAVIDROME_CLIENT=led-matrix                # any identifier
POLL_SECONDS=5